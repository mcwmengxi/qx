{
  "name": "qx",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && vue-tsc --noEmit && vite build",
    "build:noEmit": "rimraf dist && vite build",
    "build:staging": "rimraf dist && vite build --mode staging",
    "report": "rimraf dist && vite build",
    "preview:build": "pnpm build && vite preview",
    "preview": "vite preview",
    "init": "eslint --init",
    "test": "vue-tsc --noEmit ",
    "clean:cache": "rm -rf .eslintcache ",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock,build}/**/*.{vue,js,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --ignore-path .gitignore --fix",
    "prettier": "prettier --write ./**/*.{vue,ts,tsx,js,jsx,css,less,scss,json}",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@vueuse/core": "^9.3.1",
    "@vueuse/motion": "2.0.0-beta.12",
    "axios": "^1.1.3",
    "element-plus": "^2.2.18",
    "js-cookie": "^3.0.1",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.3",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.27",
    "qs": "^6.11.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@iconify-icons/ep": "^1.2.9",
    "@iconify/vue": "^4.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.2",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vitejs/plugin-legacy": "^2.3.1",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "cz-git": "^1.3.11",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mockjs": "^1.1.0",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.19",
    "postcss-import": "^15.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.8.3",
    "sass": "^1.55.0",
    "tailwindcss": "^3.2.4",
    "terser": "^5.15.1",
    "typescript": "^4.8.4",
    "unocss": "^0.45.30",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-element-plus": "^0.4.1",
    "unplugin-vue-components": "^0.22.8",
    "vite": "^3.2.4",
    "vite-plugin-cdn-import": "^0.3.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "^2.9.6",
    "vite-svg-loader": "^3.6.0",
    "vue-tsc": "^1.0.10"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,json}": "eslint --fix",
    "*.{html,css,less,scss}": [
      "prettier --write"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "webpack"
      ]
    }
  },
  "engines": {
    "node": ">=14.18.0"
  }
}
